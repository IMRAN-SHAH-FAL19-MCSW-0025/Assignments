<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="TITLE"
            android:ems="10"
            tools:layout_editor_absoluteY="253dp" tools:layout_editor_absoluteX="99dp" android:id="@+id/editText"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="FIRST NAME"
            android:ems="10"
            tools:layout_editor_absoluteY="298dp" tools:layout_editor_absoluteX="99dp" android:id="@+id/editText2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="LAST NAME"
            android:ems="10"
            tools:layout_editor_absoluteY="343dp" tools:layout_editor_absoluteX="99dp" android:id="@+id/editText3"/>
    <TextView
            android:text="PERSONAL INFO"
            android:layout_width="wrap_content"
            android:layout_height="16dp" tools:layout_editor_absoluteY="42dp"
            tools:layout_editor_absoluteX="153dp" android:id="@+id/textView"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="GENDER"
            android:ems="10"
            tools:layout_editor_absoluteY="388dp" tools:layout_editor_absoluteX="99dp" android:id="@+id/editText5"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="COUNTRY"
            android:ems="10"
            tools:layout_editor_absoluteY="433dp" tools:layout_editor_absoluteX="99dp" android:id="@+id/editText6"/>
    <TextView
            android:text="How would you like to be addressed?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="127dp"
            tools:layout_editor_absoluteX="88dp" android:id="@+id/textView2"/>

</androidx.constraintlayout.widget.ConstraintLayout>